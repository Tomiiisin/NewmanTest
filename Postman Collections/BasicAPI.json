						{
							"listen": "test",
							"script": {
								"id": "a9ecaf75-48b3-4ba4-b68c-74128ef8f3fa",
								"exec": [
									"pm.test(\"HTTP status code should be 200\", function () {",
									"    pm.response.to.have.status(200);",
									"});",
									"",
									"pm.test(\"Respone body has token\", function () {",
									"    pm.expect(pm.response.text()).to.include(\"token\");",
									"});",
									""
